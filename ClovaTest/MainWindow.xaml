<Window x:Class="ClovaTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClovaTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1100"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
    <Grid x:Name="LayoutRoot" HorizontalAlignment="Left" VerticalAlignment="Top" Height="700" Width="1100" Margin="0,0,0,0">
        <ContentControl Content="{Binding CurrentViewModel}" />
    </Grid>
</Window>
